{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "linebreak-style": 0,
    "react/jsx-props-no-spreading": ["off", {
    "html": "ignore",
    "custom": "ignore",
    "explicitSpread": "ignore",
    "exceptions": [""]
    }]
  }
}

